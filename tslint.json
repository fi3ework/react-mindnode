{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "esModuleInterop": true,
    "no-console": false
  }
}